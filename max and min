def find_max_min(numbers):
    if not numbers:
        return None, None  # Return None for both max and min if the list is empty

    max_num = min_num = numbers[0]  # Initialize max and min with the first element

    for num in numbers:
        if num > max_num:
            max_num = num
        elif num < min_num:
            min_num = num

    return max_num, min_num
    
